            <journal-title>###_Insert journal name here_###</journal-title>
        </journal-title-group>
    <!-- Editors (person)-->
        <contrib-group content-type="Editors">
            <role>Editor:</role>
            <contrib contrib-type="Editor">
                <name>
                    <surname>###_Insert editor surname here (for XML-Version)_###</surname>
                    <given-names>###_Insert editor given name here (for XML-Version)_###</given-names>
                </name>
            </contrib>
        </contrib-group>
    <!-- ISSN/ISBN -->
       <issn publication-format="online">###_Insert ISSN (online edition) here (for XML-Version)_###</issn>
